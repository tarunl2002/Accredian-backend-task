datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id        Int      @id @default(autoincrement())
  referrerName  String
  referrerEmail String
  friendName    String
  friendEmail   String
  createdAt  DateTime @default(now())
}
